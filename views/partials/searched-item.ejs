<script type="text/template" id="tmpl-searched-item">
<li rel='{{data}}'>
    <div class="searched-item-li">
        <div class="item-hdr">
        {{title}}
        </div>
        <div class="item-desc">
        {{description}}
        </div>
    </div>
</li>
</script>
<script type="text/template" id="tmpl-searched-item-air-leadprice">
    <li rel='{{data}}' style="width: 33%;">
        <div class="searched-item-li">
            <div class="item-hdr clearfix">
                <span class="left">From: {{Origin}}</span> <span class="right">To: {{Destination}}</span></div>
            <div class="item-desc clearfix">
                <p class="clearfix">
                    <span class="left">Fare:</span>
                    <span class="right">{{Fare}}</span>
                </p>
                <p class="clearfix">
                    <span class="left">Currency:</span>
                    <span class="right">{{CurrencyCode}}</span>
                </p>
                <p class="clearfix">
                    <span class="left">Departure Date:</span>
                    <span class="right">{{DepartureDateTime}}</span>
                </p>
            </div>
        </div>
    </li>
</script>
